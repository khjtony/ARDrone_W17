package playground;
import java.lang.*;
import java.util.Queue;


class Mywriter{
	public Mywriter(Queue<String> t){
		t.add("HAHAH");
	}
}

public class ThreadQueue {
	public static void main(String[] args){
		Queue<String> q = null;
		q.add("A");
		System.out.println(q.toString());
		Mywriter mywriter = new Mywriter(q);
		System.out.println(q.toString());
	}
}
